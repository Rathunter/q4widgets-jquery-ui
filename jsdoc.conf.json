{
    "opts": {
        "template": "jsdoc_template"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false,
            "outputSourcePath": false
        }
    },
    "plugins": [
        "node_modules/grunt-jsdoc/node_modules/jsdoc/plugins/markdown"
    ]
}
